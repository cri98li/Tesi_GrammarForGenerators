{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "definitions": {
        "date": {
            "type": "string",
            "pattern": "^(\\d{4}(?!\\d{2}\\b))((-)((0[1-9]|1[0-2])(\\3([12]\\d|0[1-9]|3[01]))|(00[1-9]|0[1-9]\\d|[12]\\d{2}|3([0-5]\\d|6[1-6]))))$"
        },
        "name": {
            "type": "string",
            "pattern": "^(?!.*[\u042b\u042a\u042d\u0401\u044b\u044a\u044d\u0451@%&$^#])[a-zA-Z\u0410-\u042f\u0490\u0407\u0406\u0404\u0430-\u044f\u0491\u0457\u0456\u04540-9\u2116\\\"!\\^\\*)\\]\\[(._-].*$"
        },
        "person_name": {
            "type": "string",
            "pattern": "^(?!.*[\u042b\u042a\u042d\u0401\u044b\u044a\u044d\u0451@%&$^#])[\u0410-\u042f\u0490\u0407\u0406\u0404\u0430-\u044f\u0491\u0457\u0456\u0454\\'\\- ]+$"
        },
        "gender": {
            "type": "string",
            "description": "Dictionary: GENDER"
        },
        "phone": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "description": "Dictionary: PHONE_TYPE"
                },
                "number": {
                    "type": "string",
                    "pattern": "^\\+38[0-9]{10}$"
                }
            },
            "required": [
                "type",
                "number"
            ],
            "additionalProperties": false
        },
        "document": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "description": "Dictionary: DOCUMENT_TYPE"
                },
                "number": {
                    "type": "string",
                    "minLength": 1
                },
                "issued_by": {
                    "type": "string",
                    "minLength": 1
                },
                "issued_at": {
                    "type": "string",
                    "format": "date"
                }
            },
            "required": [
                "type",
                "number"
            ],
            "additionalProperties": false
        },
        "address": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "enum": [
                        "RESIDENCE"
                    ]
                },
                "country": {
                    "type": "string"
                },
                "area": {
                    "$ref": "#/definitions/name"
                },
                "region": {
                    "$ref": "#/definitions/name"
                },
                "settlement": {
                    "$ref": "#/definitions/name"
                },
                "settlement_type": {
                    "type": "string",
                    "description": "settlement type Dictionary: SETTLEMENT_TYPE"
                },
                "settlement_id": {
                    "type": "string",
                    "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[1-5][0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}$"
                },
                "street_type": {
                    "type": "string",
                    "description": "street type Dictionary: STREET_TYPE"
                },
                "street": {
                    "$ref": "#/definitions/name"
                },
                "building": {
                    "type": "string",
                    "pattern": "^[1-9]((?![\u042b\u042a\u042d\u0401\u044b\u044a\u044d\u0451])()([\u0410-\u042f\u0490\u0407\u0406\u0404\u0430-\u044f\u0491\u0457\u0456\u0454 \\/\\'\\-0-9])){0,20}$"
                },
                "apartment": {
                    "type": "string"
                },
                "zip": {
                    "type": "string",
                    "pattern": "^[0-9]{5}$"
                }
            },
            "required": [
                "type",
                "country",
                "area",
                "settlement",
                "settlement_type",
                "settlement_id"
            ],
            "additionalProperties": false
        },
        "archive": {
            "type": "object",
            "properties": {
                "date": {
                    "$ref": "#/definitions/date"
                },
                "place": {
                    "$ref": "#/definitions/name"
                }
            }
        }
    },
    "type": "object",
    "properties": {
        "edrpou": {
            "type": "string",
            "pattern": "^[0-9]{8,10}|[0-9]{9,10}$"
        },
        "type": {
            "type": "string",
            "description": "Dictionary: LEGAL_ENTITY_TYPE_V2"
        },
        "residence_address": {
            "$ref": "#/definitions/address"
        },
        "phones": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/phone"
            },
            "minItems": 1
        },
        "email": {
            "type": "string",
            "format": "email"
        },
        "website": {
            "type": "string",
            "pattern": "^(http://|https://)?([a-z0-9][a-z0-9\\-]*\\.)+[a-z0-9][a-z0-9\\-]*$"
        },
        "receiver_funds_code": {
            "type": "string",
            "pattern": "^[0-9]+$"
        },
        "beneficiary": {
            "type": "string",
            "pattern": "^(?!.*[\u042b\u042a\u042d\u0401\u044b\u044a\u044d\u0451@%&$^#])[\u0410-\u042f\u0490\u0407\u0406\u0404\u0430-\u044f\u0491\u0457\u0456\u0454\\'\\- ]+$"
        },
        "owner": {
            "type": "object",
            "properties": {
                "first_name": {
                    "$ref": "#/definitions/person_name"
                },
                "last_name": {
                    "$ref": "#/definitions/person_name"
                },
                "second_name": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "pattern": "^(?!.*[\u042b\u042a\u042d\u0401\u044b\u044a\u044d\u0451@%&$^#])[\u0410-\u042f\u0490\u0407\u0406\u0404\u0430-\u044f\u0491\u0457\u0456\u0454\\'\\- ]+$"
                },
                "position": {
                    "type": "string",
                    "description": "Dictionary: POSITION"
                },
                "tax_id": {
                    "type": "string",
                    "pattern": "^([0-9]{9,10}|[\u0410-\u042f\u0401\u0407I\u0404\u0490]{2}\\d{6})$"
                },
                "birth_date": {
                    "$ref": "#/definitions/date"
                },
                "gender": {
                    "$ref": "#/definitions/gender"
                },
                "email": {
                    "type": "string",
                    "format": "email"
                },
                "documents": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/document"
                    },
                    "minItems": 1
                },
                "phones": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/phone"
                    },
                    "minItems": 1
                }
            },
            "required": [
                "first_name",
                "last_name",
                "position",
                "tax_id",
                "birth_date",
                "gender",
                "documents",
                "email"
            ],
            "additionalProperties": false
        },
        "accreditation": {
            "type": "object",
            "properties": {
                "category": {
                    "type": "string",
                    "description": "Dictionary: ACCREDITATION_CATEGORY"
                },
                "issued_date": {
                    "$ref": "#/definitions/date"
                },
                "expiry_date": {
                    "$ref": "#/definitions/date"
                },
                "order_no": {
                    "$ref": "#/definitions/name",
                    "description": "\u043d\u043e\u043c\u0435\u0440 \u043d\u0430\u043a\u0430\u0437\u0443 \u041c\u041e\u0417"
                },
                "order_date": {
                    "$ref": "#/definitions/date"
                }
            },
            "required": [
                "category",
                "order_no"
            ],
            "additionalProperties": false
        },
        "license": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "pattern": "^[0-9a-f]{8}(-?)[0-9a-f]{4}(-?)[0-9a-f]{4}(-?)[0-9a-f]{4}(-?)[0-9a-f]{12}$"
                },
                "type": {
                    "type": "string",
                    "description": "Dictionary: LICENSE_TYPE"
                },
                "license_number": {
                    "$ref": "#/definitions/name"
                },
                "issued_by": {
                    "$ref": "#/definitions/name"
                },
                "issued_date": {
                    "$ref": "#/definitions/date"
                },
                "expiry_date": {
                    "$ref": "#/definitions/date"
                },
                "active_from_date": {
                    "$ref": "#/definitions/date"
                },
                "what_licensed": {
                    "$ref": "#/definitions/name"
                },
                "order_no": {
                    "$ref": "#/definitions/name",
                    "description": "\u043d\u043e\u043c\u0435\u0440 \u043d\u0430\u043a\u0430\u0437\u0443"
                }
            },
            "additionalProperties": false
        },
        "status": {
            "type": "string",
            "enum": [
                "ACTIVE",
                "CLOSED"
            ]
        },
        "archive": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/archive"
            }
        },
        "security": {
            "type": "object",
            "properties": {
                "redirect_uri": {
                    "type": "string",
                    "format": "uri",
                    "pattern": "^(https)://.+"
                }
            },
            "required": [
                "redirect_uri"
            ]
        },
        "public_offer": {
            "type": "object",
            "properties": {
                "consent": {
                    "type": "boolean",
                    "enum": [
                        true
                    ]
                },
                "consent_text": {
                    "type": "string"
                }
            },
            "required": [
                "consent",
                "consent_text"
            ],
            "additionalProperties": false
        }
    },
    "required": [
        "edrpou",
        "type",
        "residence_address",
        "phones",
        "email",
        "owner",
        "security",
        "public_offer"
    ],
    "additionalProperties": false
}