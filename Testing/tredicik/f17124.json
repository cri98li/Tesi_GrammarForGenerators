{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "merchant": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string",
                    "required": true
                },
                "organizationNumber": {
                    "type": "string",
                    "required": true
                },
                "address": {
                    "type": "object",
                    "properties": {
                        "streetName": {
                            "type": "string",
                            "required": true
                        },
                        "city": {
                            "type": "string",
                            "required": true
                        },
                        "zip": {
                            "type": "string",
                            "required": true
                        }
                    },
                    "required": true
                }
            },
            "required": true
        },
        "currencySymbol": {
            "type": "string",
            "required": true
        }
    }
}